spring:
  application:
    name: EMPOLOYEE-SERVER 
server:
    port: 9001
spring: 
  datasource: 
    driverClassName: com.mysql.jdbc.Driver
    password: 
    url: "jdbc:mysql://localhost:3306/userservices"
    username: root
  jpa:
    hibernate.ddl-auto: none
    generate-ddl: false
    show-sql: true
    properties:
      hibernate:
        dialect: org.hibernate.dialect.MySQL5InnoDBDialect    
eureka: 
  client: 
    fetchRegistry: false
    registerWithEureka: false
    serviceUrl: 
      defaultZone: "http://localhost:9001/Employeeservices/"
  instance: "localhost"

